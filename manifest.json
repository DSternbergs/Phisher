{
  "manifest_version": 3,
  "name": "Phisher",
  "version": "1.0",
  "permissions": [
    "webNavigation",
    "tabs",
    "scripting",
    "activeTab",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["warning.html"],
    "matches": ["<all_urls>"]
  }],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    },
    "default_title": "Phisher"
  },

  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}

