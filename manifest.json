{
    "manifest_version": 3,
    "name": "Phisher",
    "version": "1.0",
    "description": "Detect phishing websites using heuristics and blacklists.",
    "permissions": ["webRequest", "webRequestBlocking", "activeTab", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "128.jpg",
        "48": "128.jpg",
        "128": "128.jpg"
      }
    }
  }
